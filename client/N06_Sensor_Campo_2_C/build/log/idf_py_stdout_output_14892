  825 |         "  function countOutliers(values,min,max){"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  826 |         "    const last=values.slice(-4);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  827 |         "    let out=0;"
      |         ~~~~~~~~~~~~~~~~
  828 |         "    for(const v of last){"
