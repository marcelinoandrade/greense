  830 |         "      if((isFinite(min)&&v<min)||(isFinite(max)&&v>max)) out++;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  831 |         "    }"
      |         ~~~~~~~
  832 |         "    return {out,lastCount:last.length};"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  833 |         "  }"
      |         ~~~~~  
  834 |         "  function applyWarn(cardId,canvasId,values,min,max){"
