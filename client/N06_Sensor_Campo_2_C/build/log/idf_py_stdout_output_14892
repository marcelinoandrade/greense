  719 |         /* Chart mínimo inline. Só inclua este bloco se você NÃO
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  720 |            já estiver servindo Chart.js de CDN. Se você já tem Chart.js
      |            ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  721 |            carregado em outro lugar, remova este bloco inteiro. */
      |            ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  722 |         "<script>"
      |         ~~~~~~~~~~
  723 |         "!function(){"
      |         ~~~~~~~~~~~~~~
  724 |         "function Chart(c,o){this.ctx=c;this.data=o.data;this.type=o.type;this.options=o.options||{};this._init()}"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  725 |         "Chart.prototype._init=function(){this._draw()};"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  726 |         "Chart.prototype.update=function(){this._draw()};"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  727 |         "Chart.prototype._draw=function(){"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  728 |         " const ctx=this.ctx;"
      |         ~~~~~~~~~~~~~~~~~~~~~~
  729 |         " const w=ctx.canvas.width;const h=ctx.canvas.height;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  730 |         " ctx.clearRect(0,0,w,h);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~
  731 |         " ctx.strokeStyle='#000';ctx.lineWidth=1;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  732 |         " ctx.strokeRect(0,0,w,h);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~
  733 |         " const labels=this.data.labels||[];"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  734 |         " const vals=(this.data.datasets&&this.data.datasets[0]&&this.data.datasets[0].data)||[];"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  735 |         " const lbl=(this.data.datasets&&this.data.datasets[0]&&this.data.datasets[0].label)||'';"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  736 |         " let minVal=vals.length?Math.min.apply(null,vals):0;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  737 |         " let maxVal=vals.length?Math.max.apply(null,vals):1;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  738 |         " if(isFinite(this.options.fixedMin)) minVal=this.options.fixedMin;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  739 |         " if(isFinite(this.options.fixedMax)) maxVal=this.options.fixedMax;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  740 |         " if(minVal===maxVal){maxVal=minVal+1;}"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  741 |         " const range=(maxVal-minVal)||1;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  742 |         " const ticks=4;"
      |         ~~~~~~~~~~~~~~~~~
  743 |         " ctx.strokeStyle='#ccc';"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~
  744 |         " ctx.fillStyle='#555';"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~
  745 |         " ctx.font='9px sans-serif';"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  746 |         " for(let t=0;t<=ticks;t++){"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  747 |         "   const value=minVal+(range/ticks)*t;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  748 |         "   const y=h-((value-minVal)/range)*h;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  749 |         "   ctx.beginPath();"
      |         ~~~~~~~~~~~~~~~~~~~~~
  750 |         "   ctx.moveTo(0,y);ctx.lineTo(6,y);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  751 |         "   ctx.stroke();"
      |         ~~~~~~~~~~~~~~~~~~
  752 |         "   ctx.fillText(value.toFixed(0),8,y-2);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  753 |         " }"
      |         ~~~~   
  754 |         " if(isFinite(this.options.toleranceMin)&&isFinite(this.options.toleranceMax)){"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  755 |         "   const tolMin=this.options.toleranceMin;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  756 |         "   const tolMax=this.options.toleranceMax;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  757 |         "   if(tolMin>=minVal&&tolMin<=maxVal){"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  758 |         "     const yMin=h-((tolMin-minVal)/range)*h;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  759 |         "     ctx.setLineDash([4,4]);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  760 |         "     ctx.strokeStyle='#ff9800';"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  761 |         "     ctx.lineWidth=1.5;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~
  762 |         "     ctx.beginPath();"
      |         ~~~~~~~~~~~~~~~~~~~~~~~
  763 |         "     ctx.moveTo(0,yMin);ctx.lineTo(w,yMin);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  764 |         "     ctx.stroke();"
      |         ~~~~~~~~~~~~~~~~~~~~
  765 |         "     ctx.setLineDash([]);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~
  766 |         "     ctx.fillStyle='#ff9800';"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  767 |         "     ctx.font='8px sans-serif';"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  768 |         "     ctx.fillText('Min: '+tolMin.toFixed(1),w-50,yMin-2);"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  769 |         "   }"
      |         ~~~~~~ 
  770 |         "   if(tolMax>=minVal&&tolMax<=maxVal){"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  771 |         "     const yMax=h-((tolMax-minVal)/range)*h;"
      |         ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
